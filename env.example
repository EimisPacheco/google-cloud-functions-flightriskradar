# FlightRiskRadar Environment Variables
# Copy this file to .env and replace the placeholder values with your actual API keys

# SerpAPI - For flight search data
VITE_SERPAPI_KEY=your_serpapi_key_here
SERPAPI_API_KEY=your_serpapi_key_here

# Google Gemini AI - For natural language processing
GOOGLE_API_KEY=your_google_gemini_api_key_here
VITE_GOOGLE_API_KEY=your_google_gemini_api_key_here

# OpenWeather API - For weather data
OPENWEATHER_API_KEY=your_openweather_api_key_here
VITE_OPENWEATHER_API_KEY=your_openweather_api_key_here

# Note: Google Translate and FlightAPI are not used in current AI-powered implementation

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your_google_cloud_project_id
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_GENAI_USE_VERTEXAI=true

# Cloud Function URL
VITE_CLOUD_FUNCTION_URL=https://your-cloud-function-url.run.app 